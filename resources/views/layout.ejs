    <script>
        $(document).ready(function () {

            $(window).scroll(function () {
                var postop = $(document).scrollTop();
                console.log(postop);

                if ((postop > 310) && (postop < 780)) {
                    $('#About-Animation').addClass(' animate__animated animate__fadeInUp');
                    document.documentElement.style.setProperty('--animate-duration', '2s');
                }
                else if ((postop > 1150) && (postop < 1811)) {
                    $('#BOX1').addClass(' animate__animated animate__fadeIn');
                    $('#BOX2').addClass(' animate__animated animate__fadeIn');
                    $('#BOX3').addClass(' animate__animated animate__fadeIn');
                    document.documentElement.style.setProperty('--animate-duration', '2s');
                }
                else if ((postop > 1200) && (postop < 1911)) {
                    $('#BOX4').addClass(' animate__animated animate__fadeIn');
                    $('#BOX5').addClass(' animate__animated animate__fadeIn');
                    $('#BOX6').addClass(' animate__animated animate__fadeIn');
                    document.documentElement.style.setProperty('--animate-duration', '2s');
                }

                else if ((postop > 2400) && (postop < 2711)) {
                    $('#BOX8').addClass(' animate__animated animate__fadeIn');
                    $('#BOX9').addClass(' animate__animated animate__fadeIn');
                    $('#BOX7').addClass(' animate__animated animate__fadeIn');
                    document.documentElement.style.setProperty('--animate-duration', '2s');
                }

                else if ((postop > 2800) && (postop < 3011)) {
                    $('#BOX11').addClass(' animate__animated animate__fadeIn');
                    $('#BOX12').addClass(' animate__animated animate__fadeIn');
                    $('#BOX10').addClass(' animate__animated animate__fadeIn');
                    document.documentElement.style.setProperty('--animate-duration', '2s');
                }
            });
        });

        function myFunction() {
            var x = document.getElementById("myTopnav");
            if (x.className === "topnav") {
                x.className += " responsive";
            } else {
                x.className = "topnav";
            }
        }
    </script>


