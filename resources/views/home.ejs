                                    accusamus
                                    architecto. Lorem ipsum dolor sit amet consectetur adipisicing elit. Veritatis esse
                                    vitae,
                                    eius rem eos perspiciatis voluptate consectetur totam sapiente assumenda, nostrum
                                    velit
                                    exercitationem, itaque recusandae mollitia quis architecto reprehenderit alias et.
                                    Inventore
                                  
                                    <br>
                                    <button class="Read-more" style="padding: 8px;"><a href="about-us.html"
                                            id="Read-more">READ MORE</a></button>
                                </p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <section class="food_fix" style="margin-top: 50px;">
        <div class="container">
            <div class="food_text">
                <h1>FOOD IS LOVE</h1>
            </div>
        </div>
    </section>


    <section class="menu" id="Menu">
        <div class="menu">
            <div class="container">
                <div class="col-lg-12" style="text-align: center;">
                    <h2>MENU</h2>
                    <h6 style="text-align: center;">Eat, drink and <span style="color: red;">live free </span></h6>
                    <hr>
                </div>
            </div>
        </div>
    </section>

    <section class="collage_img">
        <div class="collage">
            <div class="container">
                <div id="myBtnContainer">
                    <div class="row" style="text-align: -webkit-center; margin-top: 40px;">
                        <% Items.forEach(function(Item) { %>
                            <div class="col-lg-4 col-md-6 col-12" id="BOX1">
                                <div class="coloumn sandwich" data-item="sandwich">
                                    <div class="polaroid" tabIndex="0">
                                        <img class="img-thumbnail" src="images/<%= Item.image %>" />
                                        <div class="dish">
                                            <div class="dish-title">
                                                <h5 class="h3-title">
                                                    <%= Item.name %>
                                                </h5>
                                                <p>110 calories</p>
                                            </div>
                                            <div class="dish-bottom-row">
                                                <ul class="add-cart">
                                                    <li>
                                                        <b>Rs. <%= Item.price %></b>
                                                    </li>
                                                    <li>
                                                        <div class="col-lg-12">
                                                            <button data-item='<%= JSON.stringify(Item) %>'
                                                                class="dish-add-btn">
                                                                <i class="bi bi-plus"></i>
                                                            </button>
                                                        </div>
                                                    </li>
                                                </ul>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <% }) %>
                        </div>
